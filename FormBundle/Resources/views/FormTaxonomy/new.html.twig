{% extends "::admin.html.twig" %}
{% block title "Type de formulaire" %}
{% block titlesection "Type de formulaire" %}
{% block body %}
{% if id is not defined %}
	<form action="{{ path('new_form_taxonomy') }}" method="post" {{ form_enctype(form) }}>
{% else %}
	<form action="{{ path('edit_form_taxonomy', {'id' : id }) }}" method="post" {{ form_enctype(form) }}>
{% endif %}			
    {{ form_widget(form) }}
    <a href="{{ path('formfield_taxonomy') }}" class="btn">Fermer</a>
    <button type="submit" class="btn btn-primary">Enregistrer</button>
</form>
{% endblock body %}