{% extends "::admin.html.twig" %}
{% block title "Type de recommandation" %}

{% block menu %}
	
{% endblock menu %}

{% block titlesection %}
    {% if id is defined %}
        Editer le type de recommandation
    {% else %}
        Nouveau type de recommandation
    {% endif %} 
{% endblock titlesection %}

{% block body %}
{% if id is defined %}
	<form action="{{ path('edit_typeRecommandation', {'id' : id }) }}" method="post" {{ form_enctype(form) }}>
{% else %}
	<form action="{{ path('new_typeRecommandation') }}" method="post" {{ form_enctype(form) }}>
{% endif %}		
    {{ form_widget(form) }}
    {{ form_rest(form) }}
    {% if id is defined %}
    	<button class="btn" type="submit">Edit</button>
    {% else %}
    	<button class="btn" type="submit">Create</button>
    {% endif %}		
</form>
{% endblock body %}